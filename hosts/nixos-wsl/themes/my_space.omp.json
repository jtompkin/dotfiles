{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "arch": "\udb81\ude84",
            "windows": "\udb82\ude21"
          },
          "style": "plain",
          "template": "{{ .Icon }} ",
          "foreground": "#26C6DA",
          "background": "",
          "type": "os"
        },
        {
          "style": "plain",
          "template": "{{ .UserName }}: ",
          "foreground": "#26C6DA",
          "background": "",
          "type": "session"
        },
        {
          "properties": {
            "folder_icon": "",
            "hide_root_location": true,
            "max_depth": 2,
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "foreground": "lightGreen",
          "background": "",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "",
            "branch_identical_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }} {{ .HEAD }}{{ if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if or (.Working.Changed) (.Staging.Changed) }} \uf044 {{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "",
          "background": "",
          "type": "git"
        },
        {
          "properties": {
            "home_enabled": true
          },
          "style": "powerline",
          "template": "[\ue73c {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }} {{ .Major }}.{{ .Minor }}{{ end }}] ",
          "foreground": "#906cff",
          "background": "",
          "type": "python"
        },
        {
          "style": "powerline",
          "template": "[\ue724 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
          "foreground": "#7FD5EA",
          "background": "",
          "type": "go"
        },
        {
          "style": "powerline",
          "template": "[\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}] ",
          "foreground": "#76b367",
          "background": "",
          "type": "node"
        },
        {
          "style": "powerline",
          "template": "[\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
          "foreground": "#f44336",
          "background": "",
          "type": "ruby"
        },
        {
          "style": "powerline",
          "template": "[\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
          "foreground": "#ea2d2e",
          "background": "",
          "type": "java"
        },
        {
          "style": "powerline",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#4063D8",
          "background": "",
          "type": "julia"
        },
        {
          "style": "plain",
          "template": "‚ùØ ",
          "foreground": "#FFD54F",
          "background": "",
          "type": "text"
        }
      ]
    }
  ],
  "version": 3
}
